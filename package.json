{
  "name": "react-website-starter",
  "version": "0.0.1",
  "main": "server.bundle.js",
  "browserslist": "ie 11, edge >= 14, firefox >= 52, chrome >= 49, safari >= 10",
  "scripts": {
    "build": "webpack --config webpack/webpack.prod.js",
    "start": "node dist/server.bundle.js",
    "dev": "npm-run-all --parallel dev:server dev:client",
    "dev:server": "webpack --config webpack/webpack.dev.server.js",
    "dev:client": "webpack-dev-server --config webpack/webpack.dev.client.js",
    "prod": "npm start --production",
    "test": "jest",
    "lint": "node_modules/.bin/eslint . --ext .js --ext .jsx"
  },
  "jest": {
    "verbose": false,
    "setupFiles": [
      "./tests/setup.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/tests/**/*"
    ]
  },
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@material-ui/core": "^3.3.0",
    "apollo-cache-inmemory": "^1.3.6",
    "apollo-client": "^2.4.3",
    "apollo-link-http": "^1.5.5",
    "babel-loader": "^8.0.4",
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.18.3",
    "clean-webpack-plugin": "^0.1.19",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-beta.3",
    "express": "^4.16.4",
    "express-graphql": "^0.6.12",
    "graphql": "0.13.0",
    "graphql-tag": "^2.10.0",
    "immutable": "^3.8.2",
    "jss": "^9.8.7",
    "jss-preset-default": "^4.5.0",
    "loadable-components": "^2.2.3",
    "mobx": "^4.5.2",
    "mobx-react": "^5.3.5",
    "morgan": "^1.9.1",
    "node-fetch": "^2.2.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-apollo": "^2.2.4",
    "react-dom": "^16.5.2",
    "react-hot-loader": "^4.3.11",
    "react-immutable-proptypes": "^2.1.0",
    "react-jss": "^8.6.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-transition-group": "^2.5.0",
    "serve-favicon": "^2.5.0",
    "spdy": "^3.4.7",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.22.0",
    "webpack-merge": "^4.1.4",
    "webpack-node-externals": "^1.7.2",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@rbarilani/remove-source-map-url-webpack-plugin": "^0.1.0-alpha",
    "@types/jest": "^23.3.7",
    "apollo-link-schema": "^1.1.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "bluebird": "^3.5.2",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "graphql-tools": "^4.0.2",
    "jest": "^23.6.0",
    "jsdom": "^12.2.0",
    "npm-run-all": "^4.1.3",
    "react-test-renderer": "^16.5.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  }
}
